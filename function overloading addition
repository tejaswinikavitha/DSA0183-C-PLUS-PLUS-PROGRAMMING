#include <iostream>
using namespace std;
int add(int a, int b) {
    return a + b;
}
int add(int a, int b, int c) {
    return a + b + c;
}
int main() {
    int sum1 = add(10, 20);
    int sum2 = add(10, 20,60);
    cout << "Sum : " << sum1 << endl;
    cout << "Sum : " << sum2 << endl;

    return 0;
}
